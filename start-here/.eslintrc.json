{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "no-duplicate-imports": "off",
    "import/no-duplicates": "error"
  },
  "plugins":["import"],
  "parserOptions": {
    "ecmaVersion":2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
